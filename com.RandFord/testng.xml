		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.PageLibrary.com.RandFord.TestExecution">
				<methods>
					<include name="verify_login"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite> 
